{%- style -%}
  #shopify-section-{{ section.id }} {
    background-color: {{ section.settings.bg_color }};
  }

  .no-middlemen {
    display: flex;
    flex-direction: column;
    align-items: center;
    gap: 64px;
    padding: 80px 20px;
    max-width: 1440px;
    margin: 0 auto;
  }

  /* --- Headline --- */
  .no-middlemen__headline {
    display: flex;
    flex-direction: column;
    align-items: center;
    gap: 24px;
    text-align: center;
    max-width: 1312px;
    width: 100%;
  }

  .no-middlemen__title {
    font-family: 'Inter', sans-serif;
    font-weight: 600;
    font-size: 48px;
    line-height: 1;
    color: {{ section.settings.heading_color }};
    margin: 0;
  }

  .no-middlemen__subtitle {
    font-family: 'Inter', sans-serif;
    font-weight: 500;
    font-size: 20px;
    line-height: 1.4;
    color: {{ section.settings.subheading_color }};
    max-width: 800px;
    margin: 0;
  }

  /* --- Receipt Group --- */
  .no-middlemen__receipts {
    display: flex;
    gap: 32px;
    align-items: flex-start;
    width: 100%;
    max-width: 780px;
  }

  /* --- Single Receipt Card --- */
  .receipt {
    flex: 1;
    border-radius: 24px 24px 0 0;
    overflow: hidden;
    box-shadow: 4px 4px 12px rgba(0, 0, 0, 0.05);
    display: flex;
    flex-direction: column;
  }

  .receipt__main {
    background: #ffffff;
    display: flex;
    flex-direction: column;
    gap: 24px;
    padding: 24px 16px 40px;
  }

  .receipt__title {
    font-family: 'Geist Mono', 'Courier New', monospace;
    font-weight: 500;
    font-size: 24px;
    text-align: center;
    color: #171717;
    padding: 8px 12px 8px 20px;
    margin: 0;
  }

  .receipt__title--brand {
    font-family: 'Inter', sans-serif;
    font-weight: 600;
    font-style: normal;
  }

  /* --- Order Line Groups --- */
  .receipt__group {
    display: flex;
    flex-direction: column;
    gap: 4px;
    padding: 20px 4px;
    border-radius: 8px;
  }

  .receipt__group--highlighted {
    background-color: #ffebeb;
    border-radius: 12px;
  }

  .receipt__group-title {
    font-family: 'Geist', sans-serif;
    font-weight: 600;
    font-size: 16px;
    color: #171717;
    padding: 8px 12px 8px 20px;
    margin: 0;
  }

  /* --- Order Line --- */
  .receipt__line {
    display: flex;
    align-items: center;
    justify-content: space-between;
    gap: 10px;
    padding: 8px 12px 8px 20px;
    height: 42px;
    box-sizing: border-box;
  }

  .receipt__line-label {
    font-family: 'Geist Mono', 'Courier New', monospace;
    font-weight: 500;
    font-size: 16px;
    color: #171717;
    white-space: nowrap;
    margin: 0;
    flex-shrink: 0;
  }

  .receipt__line-label--bold {
    font-weight: 700;
  }

  .receipt__line-label--struck {
    text-decoration: line-through;
    color: #737373;
  }

  .receipt__line-dots {
    flex: 1;
    min-width: 10px;
  }

  .receipt__line-value {
    font-family: 'Geist Mono', 'Courier New', monospace;
    font-weight: 500;
    font-size: 16px;
    color: #171717;
    letter-spacing: 1.6px;
    white-space: nowrap;
    margin: 0;
    flex-shrink: 0;
  }

  .receipt__line-value--bold {
    font-weight: 700;
  }

  /* --- Free / Save Label --- */
  .receipt__label {
    font-family: 'Geist', sans-serif;
    font-weight: 600;
    font-size: 14px;
    line-height: 1;
    color: #0ea674;
    background-color: #b3f7e1;
    padding: 4px 8px;
    border-radius: 6px;
    white-space: nowrap;
    flex-shrink: 0;
  }

  /* --- Divider --- */
  .receipt__divider {
    width: 100%;
    height: 1px;
    background-color: #e5e5e5;
    margin: 0;
    border: none;
  }

  /* --- Total Line --- */
  .receipt__total {
    display: flex;
    align-items: center;
    justify-content: space-between;
    gap: 10px;
    padding: 8px 12px 8px 20px;
    height: 48px;
    box-sizing: border-box;
  }

  .receipt__total-label {
    font-family: 'Geist Mono', 'Courier New', monospace;
    font-weight: 600;
    font-size: 20px;
    color: #171717;
    line-height: 32px;
    white-space: nowrap;
    margin: 0;
    flex-shrink: 0;
  }

  .receipt__total-value {
    font-family: 'Geist', sans-serif;
    font-weight: 600;
    font-size: 20px;
    color: #171717;
    letter-spacing: 1px;
    line-height: 32px;
    white-space: nowrap;
    margin: 0;
    flex-shrink: 0;
  }

  /* --- Zig-Zag Bottom Edge --- */
  .receipt__edge {
    width: 100%;
    overflow: hidden;
    line-height: 0;
    font-size: 0;
  }

  .receipt__edge svg {
    display: block;
    width: 100%;
    height: 15px;
  }

  /* --- CTA Buttons --- */
  .no-middlemen__cta {
    display: flex;
    flex-direction: column;
    align-items: center;
    gap: 12px;
  }

  .no-middlemen__button {
    display: inline-flex;
    align-items: center;
    justify-content: center;
    background: {{ section.settings.button_bg_color }};
    color: {{ section.settings.button_text_color }};
    font-family: 'Inter', sans-serif;
    font-weight: 500;
    font-size: 14px;
    padding: 10px 24px;
    width: auto;
    height: 40px;
    border-radius: 8px;
    text-decoration: none;
    border: none;
    cursor: pointer;
    transition: opacity 0.2s;
  }

  .no-middlemen__button:hover {
    opacity: 0.9;
  }

  .no-middlemen__price-label {
    font-family: 'Inter', sans-serif;
    font-weight: 500;
    font-size: 14px;
    font-style: italic;
    color: {{ section.settings.price_label_color }};
    margin: 0;
  }

  .no-middlemen__price-label strong {
    font-weight: 700;
  }

  /* ===== MOBILE ===== */
  @media screen and (max-width: 749px) {
    .no-middlemen {
      padding: 60px 16px;
      gap: 48px;
    }

    .no-middlemen__title {
      font-size: 32px;
    }

    .no-middlemen__subtitle {
      font-size: 16px;
    }

    .no-middlemen__receipts {
      overflow-x: auto;
      flex-wrap: nowrap;
      scroll-snap-type: x mandatory;
      -webkit-overflow-scrolling: touch;
      scrollbar-width: none;
      gap: 20px;
      padding-bottom: 4px;
    }

    .no-middlemen__receipts::-webkit-scrollbar {
      display: none;
    }

    .receipt {
      flex: 0 0 270px;
      scroll-snap-align: center;
      border-radius: 12px 12px 0 0;
    }

    .receipt__main {
      gap: 10px;
      padding: 16px 12px 26px;
    }

    .receipt__title {
      font-size: 14px;
    }

    .receipt__group {
      padding: 6px 3px;
      gap: 1px;
    }

    .receipt__group--highlighted {
      border-radius: 9px;
    }

    .receipt__group-title {
      font-size: 11px;
      padding: 4px 9px 4px 15px;
    }

    .receipt__line {
      gap: 7px;
      padding: 4px 9px 4px 15px;
      height: auto;
    }

    .receipt__line-label {
      font-size: 11px;
    }

    .receipt__line-value {
      font-size: 11px;
      letter-spacing: 1px;
    }

    .receipt__label {
      font-size: 11px;
      padding: 3px 6px;
      border-radius: 4px;
    }

    .receipt__total {
      padding: 4px 9px 4px 15px;
      height: auto;
    }

    .receipt__total-label {
      font-size: 12px;
      line-height: 20px;
    }

    .receipt__total-value {
      font-size: 12px;
      line-height: 20px;
      letter-spacing: 0.66px;
    }

    .receipt__edge svg {
      height: 11px;
    }

    .no-middlemen__cta {
      width: 100%;
    }

    .no-middlemen__button {
      width: auto;
      max-width: none;
    }
  }
{%- endstyle -%}

<section class="no-middlemen" aria-label="{{ section.settings.heading }}">
  {%- comment -%} Headline {%- endcomment -%}
  <div class="no-middlemen__headline">
    <h2 class="no-middlemen__title">{{ section.settings.heading }}</h2>
    {%- if section.settings.subheading != blank -%}
      <p class="no-middlemen__subtitle">{{ section.settings.subheading }}</p>
    {%- endif -%}
  </div>

  {%- comment -%} Receipt Cards {%- endcomment -%}
  <div class="no-middlemen__receipts">
    {%- comment -%} LEFT RECEIPT: Industry Standard {%- endcomment -%}
    <div class="receipt">
      <div class="receipt__main">
        <h3 class="receipt__title">{{ section.settings.receipt_left_title }}</h3>

        {%- comment -%} Group: Middleman (highlighted) {%- endcomment -%}
        <div class="receipt__group receipt__group--highlighted">
          <p class="receipt__group-title">{{ section.settings.left_group1_title }}</p>
          {%- for block in section.blocks -%}
            {%- if block.type == 'left_middleman_line' -%}
              <div class="receipt__line" {{ block.shopify_attributes }}>
                <p class="receipt__line-label">{{ block.settings.label }}</p>
                <span class="receipt__line-dots"></span>
                <p class="receipt__line-value">{{ block.settings.value }}</p>
              </div>
            {%- endif -%}
          {%- endfor -%}
        </div>

        {%- comment -%} Group: Factory Costs {%- endcomment -%}
        <div class="receipt__group">
          <p class="receipt__group-title">{{ section.settings.left_group2_title }}</p>
          {%- for block in section.blocks -%}
            {%- if block.type == 'left_factory_line' -%}
              <div class="receipt__line" {{ block.shopify_attributes }}>
                <p class="receipt__line-label{% if block.settings.bold %} receipt__line-label--bold{% endif %}">
                  {{ block.settings.label }}
                </p>
                <span class="receipt__line-dots"></span>
                <p class="receipt__line-value{% if block.settings.bold %} receipt__line-value--bold{% endif %}">
                  {{ block.settings.value }}
                </p>
              </div>
            {%- endif -%}
          {%- endfor -%}
        </div>

        {%- comment -%} Group: Middleman delivery (highlighted) {%- endcomment -%}
        <div class="receipt__group receipt__group--highlighted">
          <p class="receipt__group-title">{{ section.settings.left_group3_title }}</p>
          {%- for block in section.blocks -%}
            {%- if block.type == 'left_delivery_line' -%}
              <div class="receipt__line" {{ block.shopify_attributes }}>
                <p class="receipt__line-label">{{ block.settings.label }}</p>
                <span class="receipt__line-dots"></span>
                <p class="receipt__line-value">{{ block.settings.value }}</p>
              </div>
            {%- endif -%}
          {%- endfor -%}
        </div>

        {%- comment -%} Divider + Total {%- endcomment -%}
        <hr class="receipt__divider">
        <div class="receipt__total">
          <p class="receipt__total-label">{{ section.settings.left_total_label }}</p>
          <span class="receipt__line-dots"></span>
          <p class="receipt__total-value">{{ section.settings.left_total_value }}</p>
        </div>
      </div>
      {%- comment -%} Zig-zag bottom edge {%- endcomment -%}
      <div class="receipt__edge">
        <svg viewBox="0 0 370 15" preserveAspectRatio="none" xmlns="http://www.w3.org/2000/svg">
          <path d="M0 0 L7.4 15 L14.8 0 L22.2 15 L29.6 0 L37 15 L44.4 0 L51.8 15 L59.2 0 L66.6 15 L74 0 L81.4 15 L88.8 0 L96.2 15 L103.6 0 L111 15 L118.4 0 L125.8 15 L133.2 0 L140.6 15 L148 0 L155.4 15 L162.8 0 L170.2 15 L177.6 0 L185 15 L192.4 0 L199.8 15 L207.2 0 L214.6 15 L222 0 L229.4 15 L236.8 0 L244.2 15 L251.6 0 L259 15 L266.4 0 L273.8 15 L281.2 0 L288.6 15 L296 0 L303.4 15 L310.8 0 L318.2 15 L325.6 0 L333 15 L340.4 0 L347.8 15 L355.2 0 L362.6 15 L370 0" fill="#ffffff" />
        </svg>
      </div>
    </div>

    {%- comment -%} RIGHT RECEIPT: Screenlux {%- endcomment -%}
    <div class="receipt">
      <div class="receipt__main">
        <h3 class="receipt__title receipt__title--brand">{{ section.settings.receipt_right_title }}</h3>

        {%- comment -%} Group: Direct Sales {%- endcomment -%}
        <div class="receipt__group">
          <p class="receipt__group-title">{{ section.settings.right_group1_title }}</p>
          {%- for block in section.blocks -%}
            {%- if block.type == 'right_sales_line' -%}
              <div class="receipt__line" {{ block.shopify_attributes }}>
                <p class="receipt__line-label{% if block.settings.struck %} receipt__line-label--struck{% endif %}">
                  {{ block.settings.label }}
                </p>
                <span class="receipt__line-dots"></span>
                {%- if block.settings.value != blank -%}
                  <p class="receipt__line-value">{{ block.settings.value }}</p>
                {%- endif -%}
                {%- if block.settings.badge != blank -%}
                  <span class="receipt__label">{{ block.settings.badge }}</span>
                {%- endif -%}
              </div>
            {%- endif -%}
          {%- endfor -%}
        </div>

        {%- comment -%} Group: Factory Costs {%- endcomment -%}
        <div class="receipt__group">
          <p class="receipt__group-title">{{ section.settings.right_group2_title }}</p>
          {%- for block in section.blocks -%}
            {%- if block.type == 'right_factory_line' -%}
              <div class="receipt__line" {{ block.shopify_attributes }}>
                <p class="receipt__line-label{% if block.settings.bold %} receipt__line-label--bold{% endif %}{% if block.settings.struck %} receipt__line-label--struck{% endif %}">
                  {{ block.settings.label }}
                </p>
                <span class="receipt__line-dots"></span>
                {%- if block.settings.value != blank -%}
                  <p class="receipt__line-value{% if block.settings.bold %} receipt__line-value--bold{% endif %}">
                    {{ block.settings.value }}
                  </p>
                {%- endif -%}
                {%- if block.settings.badge != blank -%}
                  <span class="receipt__label">{{ block.settings.badge }}</span>
                {%- endif -%}
              </div>
            {%- endif -%}
          {%- endfor -%}
        </div>

        {%- comment -%} Group: Direct Delivery {%- endcomment -%}
        <div class="receipt__group">
          <p class="receipt__group-title">{{ section.settings.right_group3_title }}</p>
          {%- for block in section.blocks -%}
            {%- if block.type == 'right_delivery_line' -%}
              <div class="receipt__line" {{ block.shopify_attributes }}>
                <p class="receipt__line-label{% if block.settings.struck %} receipt__line-label--struck{% endif %}">
                  {{ block.settings.label }}
                </p>
                <span class="receipt__line-dots"></span>
                {%- if block.settings.value != blank -%}
                  <p class="receipt__line-value">{{ block.settings.value }}</p>
                {%- endif -%}
                {%- if block.settings.badge != blank -%}
                  <span class="receipt__label">{{ block.settings.badge }}</span>
                {%- endif -%}
              </div>
            {%- endif -%}
          {%- endfor -%}
        </div>

        {%- comment -%} Divider + Total {%- endcomment -%}
        <hr class="receipt__divider">
        <div class="receipt__total">
          <p class="receipt__total-label">{{ section.settings.right_total_label }}</p>
          <span class="receipt__line-dots"></span>
          <p class="receipt__total-value">{{ section.settings.right_total_value }}</p>
          {%- if section.settings.right_total_badge != blank -%}
            <span class="receipt__label">{{ section.settings.right_total_badge }}</span>
          {%- endif -%}
        </div>
      </div>
      {%- comment -%} Zig-zag bottom edge {%- endcomment -%}
      <div class="receipt__edge">
        <svg viewBox="0 0 370 15" preserveAspectRatio="none" xmlns="http://www.w3.org/2000/svg">
          <path d="M0 0 L7.4 15 L14.8 0 L22.2 15 L29.6 0 L37 15 L44.4 0 L51.8 15 L59.2 0 L66.6 15 L74 0 L81.4 15 L88.8 0 L96.2 15 L103.6 0 L111 15 L118.4 0 L125.8 15 L133.2 0 L140.6 15 L148 0 L155.4 15 L162.8 0 L170.2 15 L177.6 0 L185 15 L192.4 0 L199.8 15 L207.2 0 L214.6 15 L222 0 L229.4 15 L236.8 0 L244.2 15 L251.6 0 L259 15 L266.4 0 L273.8 15 L281.2 0 L288.6 15 L296 0 L303.4 15 L310.8 0 L318.2 15 L325.6 0 L333 15 L340.4 0 L347.8 15 L355.2 0 L362.6 15 L370 0" fill="#ffffff" />
        </svg>
      </div>
    </div>
  </div>

  {%- comment -%} CTA Button {%- endcomment -%}
  {%- if section.settings.button_label != blank -%}
    <div class="no-middlemen__cta">
      <a
        class="btn btn-primary no-middlemen__button"
        href="{{ section.settings.button_link }}"
      >
        {{ section.settings.button_label }}
      </a>
      {%- if section.settings.price_label != blank -%}
        <p class="no-middlemen__price-label">{{ section.settings.price_label }}</p>
      {%- endif -%}
    </div>
  {%- endif -%}
</section>

